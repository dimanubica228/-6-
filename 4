fun main() {
    val unsortedArray = intArrayOf(5, 2, 9, 1, 5, 6)

    val sortedArray = bubbleSort(unsortedArray)

    println("Отсортированный массив: ${sortedArray.contentToString()}")
}

fun bubbleSort(array: IntArray): IntArray {
    val n = array.size
    for (i in 0 until n - 1) {
        for (j in 0 until n - i - 1) {
            if (array[j] > array[j + 1]) {
                // Обмен элементов
                val temp = array[j]
                array[j] = array[j + 1]
                array[j + 1] = temp
            }
        }
    }
    return array
}
